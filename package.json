{
  "name": "@hex/apollo-upload-client",
  "version": "13.0.0-beta.5",
  "description": "A terminating Apollo Link for Apollo Client that allows FileList, File, Blob or ReactNativeFile instances within query or mutation variables and sends GraphQL multipart requests.",
  "license": "MIT",
  "author": {
    "name": "Jayden Seric",
    "email": "me@jaydenseric.com",
    "url": "https://jaydenseric.com"
  },
  "repository": "github:hex-inc/apollo-upload-client",
  "homepage": "https://github.com/hex-inc/apollo-upload-client#readme",
  "bugs": "https://github.com/hex-inc/apollo-upload-client/issues",
  "funding": "https://github.com/sponsors/jaydenseric",
  "keywords": [
    "graphql",
    "multipart",
    "request",
    "file",
    "upload",
    "apollo",
    "client",
    "link",
    "react",
    "native"
  ],
  "files": [
    "lib"
  ],
  "main": "lib",
  "types": "lib/index.d.ts",
  "sideEffects": false,
  "engines": {
    "node": ">=8.10"
  },
  "browserslist": "Node >= 8.10, > 0.5%, not OperaMini all, not dead",
  "dependencies": {
    "@apollo/client": "^3.0.0-beta.38",
    "extract-files": "^5.0.1",
    "typescript": "^3.8.3"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "^2.2.1",
    "eslint": "^6.0.1",
    "eslint-config-env": "^11.0.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-compat": "^3.5.1",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-import-order-alphabetical": "^1.0.0",
    "eslint-plugin-jsdoc": "^18.1.5",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "graphql": "^14.4.2",
    "husky": "^3.0.0",
    "jsdoc-md": "^4.0.1",
    "lint-staged": "^9.1.0",
    "prettier": "^1.18.2"
  },
  "scripts": {
    "prepare": "npm run prepare:clean && npm run prepare:js && npm run prepare:jsdoc && npm run prepare:prettier",
    "prepare:clean": "rm -rf lib",
    "prepare:js": "tsc --skipLibCheck",
    "prepare:jsdoc": "jsdoc-md",
    "prepare:prettier": "prettier 'lib/**/*.js' readme.md --write",
    "test": "npm run test:eslint && npm run test:prettier && npm run test:size",
    "test:eslint": "eslint .",
    "test:prettier": "prettier '**/*.{json,yml,md}' -l",
    "test:size": "size-limit",
    "prepublishOnly": "npm test"
  }
}
